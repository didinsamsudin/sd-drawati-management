{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/server/src/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "server/src/index.js": {
            "includeFiles": "server/**"
        }
    }
}